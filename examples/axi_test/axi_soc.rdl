addrmap axi_soc #(
    longint unsigned RANDOM = 12
    ){
    subsystem;


    axi_slave slave0 @ 0x1000;
    axi_slave slave1 @ 0x2000;

    apb_subsys apb_sub @ 0x4000;

    axi_master master @ 0xfffffff0;
    axi_master master2 @ 0xffffff00;
    clk clk;
    rstn rstn;
};
